# Kraken Trading Configuration
# Set to 'false' to enable live trading (default: dry-run mode)
KRAKEN_DRY_RUN=true

# API Key Encryption
# Generate a random 32-byte key for encrypting stored API keys
# You can generate one with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_SECRET=your-32-byte-encryption-key-here

# Admin Password (optional, for future use)
# ADMIN_PASSWORD=your-secure-password-here
